{
	"name": "Fake Stack Exchange redirect",
	"short_name": "SE redirect",
	"version": "1.2.0",
	"description": "Redirect fake websites translating Stack Exchange forums questions to the official website",
	"author": "Yolopix",
	"content_scripts": [
		{
			"matches": [
				"*://qastack.fr/*/*/*",
				"*://qastack.ru/*/*/*",
				"*://qastack.mx/*/*/*",
				"*://qastack.cn/*/*/*",
				"*://qastack.com.br/*/*/*",
				"*://qastack.it/*/*/*",
				"*://qastack.vn/*/*/*",
				"*://qastack.com.de/*/*/*",
				"*://qa-stack.pl/*/*/*",
				"*://qastack.in.th/*/*/*",
				"*://qastack.info.tr/*/*/*",
				"*://qastack.co.in/*/*/*",
				"*://qastack.jp/*/*/*",
				"*://qastack.id/*/*/*",
				"*://qastack.com.ua/*/*/*",
				"*://qastack.kr/*/*/*",
				"*://qastack.net.bd/*/*/*",
				"*://qastack.lk/*/*/*"
			],
			"js": ["main.js"],
			"run_at": "document_start"
		}
	]
}
